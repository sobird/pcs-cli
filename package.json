{
  "name": "pcs-cli",
  "description": "Baidu Personal Cloud Storage's Command Line Interface.",
  "version": "0.1.9",
  "type": "module",
  "bin": {
    "pcs": "bin/pcs.js"
  },
  "scripts": {
    "dev": "rollup -cw --watch.onEnd='node ./afterBuildScript.js' --environment NODE_ENV:development",
    "start": "tsc -w",
    "build:rollup": "rollup -c --environment NODE_ENV:production",
    "build:esm": "tsc -m es6 --outDir dist/esm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:eslint": "eslint \"src/**/*.{ts,js}\" --fix",
    "preversion": "yarn lint:eslint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobird/pcs-cli.git"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "bytes": "^3.1.2",
    "chalk": "^5.3.0",
    "cliui": "^8.0.1",
    "colors": "^1.4.0",
    "commander": "^11.1.0",
    "dayjs": "^1.11.10",
    "form-data": "^4.0.0",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "osenv": "^0.1.5",
    "progress": "^2.0.3",
    "prompts": "^2.4.2"
  },
  "keywords": [
    "pcs-cli",
    "pcs",
    "baidu",
    "baidu-pcs"
  ],
  "bugs": {
    "url": "https://github.com/sobird/pcs-cli/issues"
  },
  "author": {
    "name": "sobird"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sobird/pcs-cli/blob/master/LICENSE"
    }
  ],
  "readmeFilename": "README.md",
  "homepage": "https://github.com/sobird/pcs-cli",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/bytes": "^3.1.4",
    "@types/node": "^20.9.0",
    "@types/osenv": "^0.1.3",
    "@types/progress": "^2.0.7",
    "@types/prompts": "^2.4.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "rollup": "^4.3.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
