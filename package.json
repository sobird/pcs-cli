{
  "name": "pcs-cli",
  "description": "Baidu Personal Cloud Storage's Command Line Interface.",
  "version": "1.2.2",
  "type": "module",
  "bin": {
    "pcs": "bin/pcs.ts"
  },
  "scripts": {
    "dev": "rollup -cw --watch.onEnd='node ./afterBuildScript.js' --environment NODE_ENV:development",
    "start": "tsc -w",
    "build": "rollup -c --environment NODE_ENV:production",
    "build:esm": "tsc -m es6 --outDir dist/esm",
    "test": "echo \"pre-commit scripts\"",
    "lint:eslint": "eslint \"src/**/*.{ts,js}\" --fix",
    "prepublishOnly": "tsx scripts/prepublish.ts",
    "preversion": "npm run lint:eslint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sobird/pcs-cli.git"
  },
  "keywords": [
    "pcs-cli",
    "pcs",
    "baidu",
    "baidu-pcs"
  ],
  "bugs": {
    "url": "https://github.com/sobird/pcs-cli/issues"
  },
  "author": {
    "name": "sobird"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sobird/pcs-cli/blob/master/LICENSE"
    }
  ],
  "readmeFilename": "README.md",
  "homepage": "https://github.com/sobird/pcs-cli",
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "axios": "^1.6.1",
    "bytes": "^3.1.2",
    "chalk": "^5.3.0",
    "cliui": "^8.0.1",
    "commander": "^14.0.2",
    "dayjs": "^1.11.10",
    "glob": "^13.0.0",
    "open": "^9.1.0",
    "progress": "^2.0.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/bytes": "^3.1.4",
    "@types/node": "^20.9.0",
    "@types/osenv": "^0.1.3",
    "@types/progress": "^2.0.7",
    "@types/prompts": "^2.4.8",
    "esbuild": "^0.27.2",
    "eslint-config-sobird": "^0.2.16",
    "husky": "^9.0.11",
    "rollup": "^4.54.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
